<?xml version="1.0" encoding="ASCII"?>
<routes xmlns="http://camel.apache.org/schema/spring">
    <route>
        <from uri="switchyard://InvokeService"/>
        <setHeader headerName="service">
            <simple>http://localhost:8080/attractions-provider/AttractionsService</simple>
        </setHeader>
        <log message="InvokeAG - message received: ${body} ${headers}"/>
        <to uri="switchyard://ESBService"/>
        <log message="InvokeAG - response received: ${body}"/>
    </route>
</routes>
