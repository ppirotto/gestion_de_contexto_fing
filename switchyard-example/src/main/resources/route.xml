<?xml version="1.0" encoding="ASCII"?>
<routes xmlns="http://camel.apache.org/schema/spring">
    <route>
        <from uri="switchyard://ESBService"/>
        <log message="CamelInterface - message received: ${body} ESTOY EN CAMEL!!!!!!" loggingLevel="DEBUG">
            <description>&lt;log loggingLevel="DEBUG" message="CamelInterface - message received: ${body} ESTOY EN CAMEL!!!!!!"/&gt;</description>
        </log>
        <setHeader headerName="header">
            <method beanType="com.example.switchyard.example.Header"></method>
        </setHeader>
        <routingSlip uriDelimiter=",">
            <header>header</header>
        </routingSlip>
    </route>
</routes>