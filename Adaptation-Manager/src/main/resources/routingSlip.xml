<?xml version="1.0" encoding="ASCII"?>
<routes xmlns="http://camel.apache.org/schema/spring">
    <route>
        <from uri="switchyard://RoutingService"/>
        <log message="RoutingService - message received: ${body}"/>
        <routingSlip uriDelimiter=",">
            <header>itinerary</header>
        </routingSlip>
        <log message="RoutingService - response received: ${body}"/>
    </route>
</routes>